<table class='table table-dark'>
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Votes</th>
    </tr>
  </thead>
  <tbody>
  <% @votes.each do |text, count| %>
    <tr>
      <th><%= text %></th>
      <td><%= count || 0 %>
    </tr>
  <% end %>
  </tbody>
</table>
<p><a href='/'>Cast more votes!</a></p>
